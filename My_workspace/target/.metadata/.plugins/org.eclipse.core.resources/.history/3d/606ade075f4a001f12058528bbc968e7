#if !defined(__SOFT_FP__) && defined(__ARM_FP)
  #warning "FPU is not initialized, but the project is compiling for an FPU. Please initialize the FPU before use."
#endif

int main(void)
{
	volatile uint32_t *pRCC_AHB1ENR = (uint32_t*)0x40023830;
	volatile uint32_t *pRCC_CFGR = (uint32_t*)0x40023808;
	volatile uint32_t *pGPIOAMode = (uint32_t*)0x40020000;
	volatile uint32_t *pGPIOAOutput = (uint32_t*)0x40020014;

	// Set MCO clock as HSI
	*pRCC_CFGR |= ~(3<<21);

	// Enable GPIOA clock
	*pRCC_AHB1ENR |= (1<<0);

	// Set PA8 as output
	*pGPIOAMode &= ~(3<<16);
	*pGPIOAMode |= (1<<16);






